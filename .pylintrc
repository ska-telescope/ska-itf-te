[MESSAGES CONTROL]
disable=
    duplicate-code,
    fixme,
    # try-except-raise is a great way to write self-documenting code!
    try-except-raise,
    logging-fstring-interpolation,
    too-many-arguments,
    unused-argument,
    too-few-public-methods
    

enable=
    useless-suppression,

[SIMILARITIES]
min-similarity-lines=7
ignore-imports=yes
